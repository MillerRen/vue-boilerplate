{
  "name": "vue-boilerplate",
  "version": "1.1.0",
  "description": "Vue 2.0 boilerplate includes vuex vue-router vue-resource vue-auth",
  "author": "Miller Ren <ren12345@126.com>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "deploy": "npm run build && npm run gh-pages",
    "gh-pages": "gh-pages -d dist --remote origin",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags && npm run gh-pages"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "nprogress": "^0.2.0",
